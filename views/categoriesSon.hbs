<div class="container">
<div class="row">
<div class="col-md-8">


<div class="row">
<br>
    {{#each this.mobile}}
        <div class="category_article_wrapper">
        <div class="row">
            <div class="col-md-5">
                <div class="top_article_img">
                    <a href="/categories/{{Name}}/{{Id}}" target="_self">
                        <img style="height: 200px" class="img-responsive" src="{{Thumbnail}}" alt="feature-top">
                    </a>
                </div>
                <!-- top_article_img -->

            </div>
            <div class="col-md-7">
                <span class="tag orange">{{Name}}</span>
                <span class="tag orange">{{NameSon}}</span>
                <div class="category_article_title">
                    <h2><a href="/categories/{{Name}}/{{Id}}" target="_self">
                        {{Title}}
                    </a></h2>
                </div>
                <!-- category_article_title -->

                <div class="article_date"><a href="#">{{Created_date}}</a>, Người đăng: <a href="#">{{Author}}</a></div>
                <!----article_date------>
                <!-- category_article_wrapper -->

                <div class="category_article_content">
                    {{Summary}}
                </div>
                <!-- category_article_content -->

                <div class="media_social">
                    <span><a href="#"><i class="fa fa-share-alt"></i>{{Count_Like}} </a> Like</span>
                    <span><i class="fa fa-comments-o"></i><a href="#">4</a> Bình luận</span>
                </div>
                <!-- media_social -->

            </div>
            <!-- col-md-7 -->

        </div>
        <!-- row -->

    </div>
    <!-- category_article_wrapper -->
    {{/each}}

</div>
<!-- row -->
<div class="widget_advertisement"></div>

<nav aria-label="Page navigation" class="pagination_section">
    <ul class="pagination">
        <li class="page-item disabled">
            <a class="page-link" href="#" tabindex="-1" aria-disabled="true">Previous</a>
        </li>
        {{#each this.pages}}
            {{#if active}}
                <li class="page-item active" aria-current="page">
                    <a class="page-link" href="javascript:;">{{value}} <span class="sr-only">(current)</span></a>
                </li>
            {{else}}
                <li class="page-item">
                    <a class="page-link" href="?page={{value}}">{{value}}</a>
                </li>
            {{/if}}
        {{/each}}
        <li class="page-item">
        <a class="page-link" href="#">Next</a>
        </li>
    </ul>
</nav>
<!-- navigation -->

</div>
<!-- col-md-8 -->

<div class="col-md-4">
<div class="widget">
    <div class="widget_title widget_black">
        <h2><a href="#">New posts</a></h2>
    </div>

    {{#each this.newPost}}
    <div class="media">
        <div class="media-left">
            <a href="/categories/{{Name}}/{{Id}}"><img style="width: 100px; height: 100px" class="media-object" src="{{Thumbnail}}" alt="Generic placeholder image"></a>
        </div>
        <div class="media-body">

            <span class="tag orange">{{Name}}</span>

            <h3 class="media-heading">
                <a href="/categories/{{Name}}/{{Id}}" target="_self">{{Title}}</a>
            </h3> <span class="media-date"><a href="#">{{Created_date}}</a>,  Người đăng: <a href="#">{{Author}}</a></span>

            <div class="widget_article_social">
                <span>
                    <a href="#" target="_self"> <i class="fa fa-share-alt"></i>{{Count_Like}}</a> Like
                </span> 
                <span>
                    <a href="#" target="_self"><i class="fa fa-comments-o"></i>4</a> Bình luận
                </span>
            </div>
        </div>
    </div>
    {{/each}}
    

    {{!-- <p class="widget_divider"><a href="#" target="_self">Xem thêm&nbsp;&raquo;</a></p> --}}
</div>
<!-- Popular News -->


